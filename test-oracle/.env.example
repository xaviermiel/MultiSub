# RPC URL for Ethereum Sepolia
RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Private key of the authorized updater (must match AUTHORIZED_UPDATER in the module)
PRIVATE_KEY=0x...

# DeFiInteractorModule contract address
MODULE_ADDRESS=0x70778aD876eE8964218149b93f521E681C3CB90f

# Cron schedules
SAFE_VALUE_CRON="*/30 * * * * *"
SPENDING_ORACLE_CRON="*/5 * * * *"

# Event polling interval in milliseconds (default 10 seconds)
POLL_INTERVAL_MS=10000

# Blocks to look back for historical events (~24h on Ethereum = 7200 blocks)
BLOCKS_TO_LOOK_BACK=7200

# Window duration in seconds (default 24 hours)
WINDOW_DURATION_SECONDS=86400

# Gas limit for transactions
GAS_LIMIT=500000
